{
  "name": "@caravanapp/web-api",
  "version": "1.0.0",
  "description": "Web API for Caravan's buddy reading website",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run build:clean && tsc",
    "build:clean": "rimraf ./dist",
    "build:mongo": "cd ../mongo && npm run build",
    "start": "cross-env NODE_ENV=production node ./dist/index.js",
    "dev": "cross-env NODE_ENV=development npm run build:mongo && nodemon src/index.ts",
    "dev:prebuilt": "cross-env NODE_ENV=development nodemon --inspect src/index.ts",
    "dev:watch": "cross-env NODE_ENV=development npm run build:mongo && nodemon --inspect src/index.ts",
    "lint": "eslint \"src/**/*\"",
    "lint:fix": "eslint \"src/**/*\" --quiet --fix",
    "test": "jest"
  },
  "repository": "https://github.com/caravanapp-ca/caravanapp-ca/tree/master/packages/web-api",
  "keywords": [
    "caravan",
    "buddy",
    "reading",
    "api"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/caravanapp-ca/caravanapp-ca/issues"
  },
  "homepage": "https://github.com/caravanapp-ca/caravanapp-ca#README",
  "dependencies": {
    "@caravanapp/mongo": "1.0.0",
    "@google-cloud/pubsub": "^1.7.0",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "btoa": "^1.2.1",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^1.4.0",
    "cross-env": "^7.0.2",
    "date-fns": "^2.11.1",
    "discord.js": "^12.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.4.0",
    "form-data": "^3.0.0",
    "fuse.js": "^3.4.5",
    "helmet": "^3.22.0",
    "mongoose": "~5.9.4",
    "node-fetch": "^2.6.0",
    "slugify": "^1.4.0",
    "uuid": "^7.0.2"
  },
  "devDependencies": {
    "@caravanapp/types": "1.0.0",
    "@types/btoa": "^1.2.3",
    "@types/cookie-parser": "^1.4.2",
    "@types/cookie-session": "^2.0.41",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.8",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^25.1.4",
    "@types/mongoose": "5.7.9",
    "@types/node": "^12.12.34",
    "@types/node-fetch": "^2.5.7",
    "@types/uuid": "^8.3.0",
    "@types/ws": "^7.2.6",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "24.9.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "ts-jest": "24.1.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2",
    "utility-types": "^3.10.0"
  }
}
