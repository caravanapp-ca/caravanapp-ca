{
  "name": "@caravanapp/web-api",
  "version": "1.0.0",
  "description": "Web API for Caravan's buddy reading website",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run build:clean && tsc",
    "build:clean": "rimraf ./dist",
    "build:mongo": "cd ../mongo && npm run build",
    "start": "cross-env NODE_ENV=production node ./dist/index.js",
    "dev": "cross-env NODE_ENV=development npm run build:mongo && nodemon src/index.ts",
    "dev:prebuilt": "cross-env NODE_ENV=development nodemon --inspect src/index.ts",
    "dev:watch": "cross-env NODE_ENV=development npm run build:mongo && nodemon --inspect src/index.ts",
    "lint": "eslint \"src/**/*\"",
    "lint:fix": "eslint \"src/**/*\" --quiet --fix",
    "test": "jest"
  },
  "repository": "https://github.com/caravanapp-ca/caravanapp-ca/tree/master/packages/web-api",
  "keywords": [
    "caravan",
    "buddy",
    "reading",
    "api"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/caravanapp-ca/caravanapp-ca/issues"
  },
  "homepage": "https://github.com/caravanapp-ca/caravanapp-ca#README",
  "dependencies": {
    "@caravanapp/mongo": "1.0.0",
    "@google-cloud/pubsub": "^1.7.0",
    "axios": "^0.26.0",
    "body-parser": "^1.19.2",
    "btoa": "^1.2.1",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "date-fns": "^2.11.1",
    "discord.js": "^12.1.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-validator": "^6.4.0",
    "form-data": "^3.0.0",
    "fuse.js": "^3.4.5",
    "helmet": "^5.0.2",
    "mongoose": "~5.9.4",
    "node-fetch": "^3.2.0",
    "slugify": "^1.4.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@caravanapp/types": "1.0.0",
    "@types/btoa": "^1.2.3",
    "@types/cookie-parser": "^1.4.2",
    "@types/cookie-session": "^2.0.44",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/mongoose": "5.7.9",
    "@types/node": "^16.11.25",
    "@types/node-fetch": "^2.6.1",
    "@types/uuid": "^8.3.0",
    "@types/ws": "^7.2.6",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "27.5.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "27.1.3",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5",
    "utility-types": "^3.10.0"
  }
}
